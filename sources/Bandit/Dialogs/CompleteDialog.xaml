<UserControl x:Class="Bandit.Dialogs.CompleteDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Bandit.Dialogs"
             mc:Ignorable="d" 
             d:DesignHeight="120" d:DesignWidth="220">
    <StackPanel Orientation="Vertical" VerticalAlignment="Center" Margin="20">
        <materialDesign:PackIcon Kind="TooltipPerson" HorizontalAlignment="Center" Width="30" Height="30" Opacity=".68"/>
        <TextBlock Style="{StaticResource MaterialDesignBody2TextBlock}" HorizontalAlignment="Center" Text="작업이 완료되었습니다!" Opacity=".68"/>
    </StackPanel>
</UserControl>
